{
  "env": {
    "es2021": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "eslint:recommended",
    "google",
    "plugin:prettier/recommended",
    "plugin:security/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "settings": {
    "jsdoc": {
      "mode": "typescript"
    }
  },
  "plugins": ["@typescript-eslint", "security"],
  "rules": {
    "prettier/prettier": "warn",
    "require-jsdoc": "off"
  }
}
